{
  "name": "hextraction-catalog",
  "private": true,
  "version": "1.0.0",
  "description": "Hextraction Catalog",
  "repository": "https://github.com/cadeluca/hextraction-catalog",
  "type": "module",
  "author": "cadeluca",
  "license": "MIT",
  "scripts": {
    "gen:mdx": "node ./src/data/generateMdxFiles.js",
    "gen:data": "node ./src/data/updateTiles.js",
    "gen:db": "node ./src/data/setupDb.js",
    "gen:full": "npm run gen:db && gen:data && run gen:mdx",
    "build:full": "npm run gen:full && next build",
    "build": "next build",
    "postbuild": "next-sitemap && pagefind --site .next/server/app --output-path public/_pagefind",
    "dev": "next --turbopack",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "googleapis": "^144.0.0",
    "lucide": "^0.479.0",
    "lucide-react": "^0.479.0",
    "next": "^15.3.1",
    "nextra": "^4.2.17",
    "nextra-theme-docs": "^4.2.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "4.1.4",
    "@types/node": "22.8.1",
    "@types/react": "19.0.10",
    "autoprefixer": "^10.4.20",
    "next-sitemap": "^4.2.3",
    "pagefind": "^1.3.0",
    "tailwindcss": "^4.1.4"
  }
}
